{
  "name": "born-to-sell",
  "version": "1.0.0",
  "description": "React & gitbooks reading for selling covered calls",
  "main": "build",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run check",
    "postversion": "git push && git push --tags && npm run clean && npm run docs:publish",
    "version": "npm run build",
    "clean": "rimraf build",
    "lint": "eslint src/**",
    "check": "npm run lint && npm run test:all",
    "build": "babel src --out-dir build",
    "dev": "npm install && npm run clean && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "mocha 'test/**/*.js'",
    "test:only": "mocha",
    "test:single": "mocha --watch",
    "test:watch": "mocha --watch",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m 'update book' && git fetch origin gh-pages && git checkout -b gh-pages && git add . && git commit -am 'update book' && git push origin gh-pages --force",
    "travis": "babel-node ./node_modules/.bin/istanbul cover --report html _mocha --"
  },
  "repository": {
    "type": "git",
    "url": "https://gnagel@github.com/ctrl-alt-p/born-to-sell.git"
  },
  "keywords": [
    "javascript",
    "stocks",
    "options",
    "covered calls",
    "derivatives",
    "test",
    "reactjs",
    "react",
    "flux",
    "testing",
    "test utils",
    "assertion helpers",
    "tdd",
    "mocha"
  ],
  "author": "Glenn Nagel <glenn.nagel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-stage-1": "^6.5.0",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redbox-react": "^1.2.2",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "coveralls": "^2.11.8",
    "enzyme": "^2.1.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "flow-bin": "^0.22.1",
    "gitbook-cli": "^2.1.2",
    "istanbul": "^0.4.2",
    "jquery": "^2.2.2",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-catch-errors": "^1.0.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {}
}
